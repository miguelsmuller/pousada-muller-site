{
  "name": "pousada-muller-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=local",
    "build": "ng build --build-optimizer --vendor-chunk --source-map=false",
    "build:local": "ng build --aot --prod --build-optimizer --vendor-chunk --source-map=false",
    "build:prod": "ng build --aot --prod --build-optimizer --vendor-chunk --source-map=false --deploy-url=https://miguelsmuller.github.io/pousada-muller-site/ --base-href=/pousada-muller-site/",
    "test": "ng test",
    "lint": "ng lint ./src/app",
    "e2e": "ng e2e",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.11",
    "@angular/cdk": "~11.2.10",
    "@angular/common": "~11.2.11",
    "@angular/compiler": "~11.2.11",
    "@angular/core": "~11.2.11",
    "@angular/forms": "~11.2.11",
    "@angular/material": "~11.2.10",
    "@angular/material-moment-adapter": "~12.0.0",
    "@angular/platform-browser": "~11.2.11",
    "@angular/platform-browser-dynamic": "~11.2.11",
    "@angular/router": "~11.2.11",
    "moment": "~2.29.1",
    "ngx-mask": "^12.0.0",
    "ngx-picture": "~2.0.14",
    "rxjs": "~6.6.0",
    "swiper": "~6.7.0",
    "tslib": "~2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.10",
    "@angular-eslint/builder": "~4.3.0",
    "@angular-eslint/eslint-plugin": "~4.3.0",
    "@angular-eslint/eslint-plugin-template": "~4.3.0",
    "@angular-eslint/template-parser": "~4.3.0",
    "@angular/cli": "~11.2.10",
    "@angular/compiler-cli": "~11.2.11",
    "@ngneat/tailwind": "~7.0.3",
    "tailwindcss": "~2.1.2",
    "@tailwindcss/aspect-ratio": "~0.2.0",
    "@tailwindcss/forms": "~0.3.2",
    "@tailwindcss/line-clamp": "~0.2.0",
    "@tailwindcss/typography": "~0.4.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~12.11.1",
    "@typescript-eslint/eslint-plugin": "~4.16.1",
    "@typescript-eslint/parser": "~4.16.1",
    "autoprefixer": "~10.2.5",
    "eslint": "~7.6.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-prettier": "~3.4.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.5.0",
    "postcss": "~8.2.13",
    "prettier": "~2.3.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.2"
  }
